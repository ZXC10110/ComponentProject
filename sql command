CREATE TABLE CovidCaseAllP (
    id int PRIMARY KEY NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
    years int ,
    weeknum int,
    province varchar(255),
    new_case int,
    total_case int,
    new_case_excludeabroad int,
    total_case_excludeabroad int,
    new_death int,
    total_death int,
    update_date varchar(255)
)



CREATE TABLE CovidRound1To2All (
    years int ,
    weeknum int,
    new_case int,
    total_case int,
    new_case_excludeabroad int,
    total_case_excludeabroad int,
    new_death int,
    total_death int,
    update_date varchar(255),
    new_recovered int,
    total_recovered int,
    case_foreign int,
    case_prison int,
    case_walkin int,
    case_new_prev int,
    case_new_diff int,
    death_new_prev int,
    death_new_diff int
);

